{
  "version": 2,
  "outputDirectory": ".",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://minzei-tax.jp/",
      "statusCode": 301,
      "has": [
        {
          "type": "host",
          "value": "(www\\.)?minzei-tax\\.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/chat",
      "destination": "/chat.html"
    },
    {
      "source": "/chatbot",
      "destination": "/chatbot.html"
    },
    {
      "source": "/cost",
      "destination": "/cost.html"
    },
    {
      "source": "/cost.tax",
      "destination": "/cost.tax.html"
    },
    {
      "source": "/1218tst",
      "destination": "/1218tst.html"
    },
    {
      "source": "/keiyaku.html",
      "destination": "/1218tst.html"
    },
    {
      "source": "/keiyaku",
      "destination": "/1218tst.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
